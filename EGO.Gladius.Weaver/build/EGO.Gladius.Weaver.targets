<Project>
	<Target Name="RunAfterBuild" AfterTargets="Build;Rebuild;Publish">
		<ItemGroup>
			<OutputDlls Include="$(TargetDir)**\*.dll" />
			<OutputDlls Include="$(PublishDir)**\*.dll" Condition="Exists('$(PublishDir)')" />
		</ItemGroup>
		<Exec Command="dotnet exec &quot;$(MSBuildThisFileDirectory)..\tools\net10.0\any\EGO.Gladius.Weaver.dll&quot; @(BuiltDlls->'&quot;%(FullPath)&quot;', ' ')" />
	</Target>
	
</Project>